<div id="page-title"><img id="minpad-right" src="images/small-green.png" alt="Small Green Cube"/><img id="minpad-left" src="images/small-white.png" alt="Small White Cube"/><h2>Small Cubes</h2><img id="minpad-right" src="images/small-brown.png" alt="Small Brown Cube"/><img id="minpad-left" src="images/small-unity.png" alt="Small Unity Wild Cube"/></div>
<% for(let i = 0; i < playerData.length; ++i) { %>
    <div id="hwrapper">
        <input type="text" id="player-name-<%= i %>" value="<%= playerData[i].name %>" placeholder="Player <%= i + 1 %> Name" tabindex="<%= (i+1) %>" />
        <%= renderResourceInput(i) %>
    </div>
<% } %>
    <div id="hwrapper">
        <button onclick="setNames(); setSmallCubes(); prev();">Back</button>
        <button onclick="setNames(); setSmallCubes(); next();">Next</button>
    </div>

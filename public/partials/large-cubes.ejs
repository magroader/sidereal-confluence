<div id="page-title"><img src="images/large-black.png" alt="Large Black Cube"/><img src="images/large-blue.png" alt="Large Blue Cube"/><h2>Large Cubes</h2><img src="images/large-yellow.png" alt="Large Yellow Cube"/><img src="images/large-unity.png" alt="Large Unity Wild Cube"/></div>
<% for(let i = 0; i < playerData.length; ++i) { %>
    <div id="hwrapper">
        <input type="text" id="player-name-<%= i %>" value="<%= playerData[i].name %>" placeholder="Player <%= i + 1 %> Name" tabindex="<%= (i+1) %>" />
        <input type="number" id="player-resource-<%= i %>" value="<%= playerData[i].largeCubes %>" placeholder="0" min="0" tabindex="<%= (i+1+playerData.length) %>"  <% if (i===0) { %> autofocus <% } %>  />
    </div>
<% } %>
    <div id="hwrapper">
        <button onclick="setNames(); setLargeCubes(); prev();">Back</button>
        <button onclick="setNames(); setLargeCubes(); next();">Next</button>
    </div>
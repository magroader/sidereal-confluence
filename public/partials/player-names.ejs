<div id="page-title"><img src="images/colony.png" alt="Colony Icon"/><h2>Player Names</h2><img src="images/technology.png" alt="Technology Icon"/></div>
<% for(let i = 0; i < playerData.length; ++i) { %>
    <div id="hwrapper">
        <input type="text" id="player-name-<%= i %>" value="<%= playerData[i].name %>" placeholder="Player <%= i + 1 %> Name" tabindex="<%= (i+1) %>" <% if (i===0) { %> autofocus <% } %> />
    </div>
<% } %>
    <div id="hwrapper">
        <button onclick="setNames(); prev();">Back</button>
        <button onclick="setNames(); next();">Next</button>
    </div>